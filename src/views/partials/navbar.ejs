<div class="navbar">
  <% const links = [{Name: "Home", Anchor: "/home", Id: "home"}, {Name: "Products", Anchor: "/products/all", Id: "products"}, {Name: "Contact Us", Anchor: "/contact", Id: "contact"}, {Name: "Updates", Anchor: "/updates", Id: "updates"}, {Name: "Offers", Anchor: "/offers", Id: "offers"}, {Name: "Locate Us", Anchor: "/locate", Id: "locate"} ] %>

  <a class="logo-anchor" href="/">
    <span class="logo">
      Kalawati Enterprises
    </span>
  </a>

  <span class="links">
    <% links.forEach(link => { %>
      <a href="<%= link.Anchor %>" class="<%= activePage === link.Id ? 'active' : '' %>">
        <%= link.Name %>
      </a>
    <% }) %>
  </span>
</div>
